    "ajv": "^6.12.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "jquery": "^3.4.1",
    "mini-css-extract-plugin": "^0.4.5",
    "prettier": "^2.2.1",
    "terminalizer-player": "^0.4.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "xterm": "^3.14.5"
  }
